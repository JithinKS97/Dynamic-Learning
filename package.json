{
  "name": "dynamic-learning",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "test": "meteor test --once --full-app --driver-package meteortesting:mocha",
    "full-test": "TEST_WATCH=1 meteor test --driver-package meteortesting:mocha",
    "lint": "eslint .",
    "coverage:watch": "BABEL_ENV=COVERAGE COVERAGE=1 COVERAGE_VERBOSE=1 COVERAGE_APP_FOLDER=$PWD/ TEST_WATCH=1 meteor test --driver-package meteortesting:mocha"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "bcrypt": "^3.0.7",
    "dompurify": "^2.0.7",
    "fabric": "^3.5.1",
    "history": "^4.10.1",
    "meteor-node-stubs": "^0.4.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
    "react-color": "^2.17.3",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-mathquill": "^0.1.9",
    "react-meteor-data": "^0.2.11",
    "react-rnd": "^10.1.1",
    "react-router-dom": "^5.1.2",
    "react-sizeme": "^2.6.10",
    "react-sortable-hoc": "^1.10.1",
    "react-sortable-tree": "^2.7.1",
    "react-sortable-tree-theme-file-explorer": "^2.0.0",
    "react-tagsinput": "^3.19.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1",
    "simpl-schema": "^1.5.6"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-meteor": "^6.0.0",
    "eslint-plugin-react": "^7.16.0",
    "mocha": "^6.2.2"
  },
  "engines": {
    "node": "8.11.1"
  },
  "eslintConfig": {
    "extends": "@meteorjs/eslint-config-meteor"
  },
  "babel": {
    "env": {
      "COVERAGE": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  }
}
